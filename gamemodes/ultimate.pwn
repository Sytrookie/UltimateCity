public ABroadCast(color,const string[],level)
{
	foreach (Player, i)
	{
		if(gPlayerLogged[i] == 1)
		{
		    if(level==-1) {
		    if(PlayerInfo[i][pAdmin]>1&&!PlayerInfo[i][pHelper]) {
  				SendClientMessage(i, color, string);
		    }
		    } else if ((IsPlayerAdmin(i)) || PlayerInfo[i][pAdmin] >= level)
			{
				SendClientMessage(i, color, string);
				//printf("%s", string);
			}
		}
	}
	return 1;
}

