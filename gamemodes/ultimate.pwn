public LoadIRC()
{
	new arrCoords[6][64];
	new row[256];
	new string[128];
	new idx;
	printf("Loading IRC channels");
	while (idx < sizeof(IRCInfo))
	{
	    format(string, sizeof(string), "SELECT * FROM `channels` WHERE `ID`=%d", idx);
		mysql_query_ex(string);
		mysql_store_result();
		if(mysql_num_rows() > 0)
	    {
			mysql_fetch_row(row);
			split(row, arrCoords, '|');
			mysql_free_result();
			strmid(IRCInfo[idx][iAdmin], arrCoords[1], 0, strlen(arrCoords[2]), 255);
			strmid(IRCInfo[idx][iMOTD], arrCoords[2], 0, strlen(arrCoords[2]), 255);
			strmid(IRCInfo[idx][iPassword], arrCoords[3], 0, strlen(arrCoords[3]), 255);
			IRCInfo[idx][iNeedPass] = strval(arrCoords[4]);
			IRCInfo[idx][iLock] = strval(arrCoords[5]);
		}
    	mysql_free_result();
		idx++;
	}
	printf("IRC channels loaded.");
	return 1;
}

public SaveIRC()
{
	new idx;
	new string[256];
	printf("Saving IRC channels");
	while (idx < sizeof(IRCInfo))
	{
		format(string, sizeof(string), "UPDATE `channels` SET `Admin`='%s', `MOTD`='%s', `Password`='%s', `NeedPass`=%d, `Lock`=%d WHERE `ID`=%d", IRCInfo[idx][iAdmin],IRCInfo[idx][iMOTD],IRCInfo[idx][iPassword],IRCInfo[idx][iNeedPass],IRCInfo[idx][iLock],idx);
		mysql_query_ex(string);
		idx++;
	}
	printf("IRC channels saved.");
	return 1;
}

