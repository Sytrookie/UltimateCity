public SetPlayerUnjail()
{
	//each second fits with this - robbiz timer, but it doesn't have anything to do with unjailing player
	if(CanRob > 0)
	{
	    CanRob --;
	}
	foreach (Player, i)
	{
		if(GetPVarInt(i, "StepsAD") > 0)
		{
	    	SetPVarInt(i, "StepsAD", GetPVarInt(i, "StepsAD") - 1);
	    }
	    if(RobTimer[i] > 1)
	    {
	        if(RobTimer[i] == 55) SendClientMessage(i, COLOR_RED, "55 seconds left!");
	        if(RobTimer[i] == 50) SendClientMessage(i, COLOR_RED, "50 seconds left!");
	        if(RobTimer[i] == 40) SendClientMessage(i, COLOR_ORANGE, "40 seconds left!");
	        if(RobTimer[i] == 30) SendClientMessage(i, COLOR_ORANGE, "30 seconds left!");
	        if(RobTimer[i] == 20) SendClientMessage(i, COLOR_YELLOW, "20 seconds left!");
	        if(RobTimer[i] == 10) SendClientMessage(i, COLOR_GREEN, "10 seconds left!");
	        RobTimer[i] --;
	    }
	    else if(RobTimer[i] == 1)
	    {
            if(IsPlayerInRangeOfPoint(i, 30.0, RPx[i], RPy[i], RPz[i]))
            {
                if(CanRob != 0)
                {
                    SendClientMessage(i, COLOR_WHITE, "Someone robbed a business before you, better luck next time!");
                    return 1;
                }
                else
                {
                    CanRob = 3600;//60 minutes
                    RobTimer[i] = 0;
                    RPx[i] = 0.0;
                    RPy[i] = 0.0;
                    RPz[i] = 0.0;
                    ShowPlayerDialog(i, ROB_DIALOG, DIALOG_STYLE_LIST,"Money Laundering:", "SanFierro \nLas Venturas", "Select", "Select");
                }
            }
            else
            {
                SendClientMessage(i, COLOR_WHITE, "You aren't near the robbusiness position, robbery canceled!");
            }
	    }
		if(PlayerInfo[i][pJailTime] > 0)
		{
			PlayerInfo[i][pJailTime]--;
			if(PlayerInfo[i][pJailTime] == 0)
			{
		    	PlayerInfo[i][pJailed] = 0;
		    	SetPlayerPos(i, 1550.5609,-1674.4252,15.4549);
		    	SetPlayerInterior(i,0);
		    	SendClientMessage(i, COLOR_GRAD1,"   You have paid Your debt to society.");
            	GameTextForPlayer(i, "~g~Freedom~n~~w~Try to be a better citizen", 5000, 1);
		    	TogglePlayerControllable(i,true);
		    	SetPlayerVirtualWorld(i,0);
		    	Mute[i] = 0;
			}
		}
		if(PlayerInfo[i][pJailTime] == 0 && PlayerInfo[i][pJailed] > 0)
		{
		    PlayerInfo[i][pJailed] = 0;
		    SetPlayerPos(i, 1550.5609,-1674.4252,15.4549);
		    SetPlayerInterior(i,0);
		    SendClientMessage(i, COLOR_GRAD1,"   You have paid Your debt to society.");
            GameTextForPlayer(i, "~g~Freedom~n~~w~Try to be a better citizen", 5000, 1);
		    TogglePlayerControllable(i,true);
	        SetPlayerVirtualWorld(i,0);
		    Mute[i] = 0;
		}
	}
	return 1;
}

