forward RemoveBan(ip[]);
public RemoveBan(ip[])
{
	for(new idx; idx<sizeof(BanInfo); idx++)
	{
		if(!strcmp(BanInfo[idx][BanInfoIP],ip,true))
		{
		    BanInfo[idx][BanInfoTS]=0;
		    BanInfo[idx][BanInfoActive]=0;
		    SaveBans();
		    return 1;
		}
	}
	return 0;
}
forward AddBan(playerid,time,result[]);
public AddBan(playerid,time,result[]) {
	new idx,ip[16];
	GetPlayerIp(playerid,ip,sizeof(ip));
	while(idx<sizeof(BanInfo)) {
		if(BanInfo[idx][BanInfoTS]==0) {
			strmid(BanInfo[idx][BanInfoIP], ip, 0, strlen(ip), 19);
			BanInfo[idx][BanInfoTS] = GetUnixTimeDays(time);
			strmid(BanInfo[idx][BanInfoReason], result, 0, strlen(result), 128);
			BanInfo[idx][BanInfoActive]=1;
			SaveBans();
			return 1;
		}
	    idx++;
	}
	return 0;

}
forward IsIPBanned(playerid);
public IsIPBanned(playerid) {
	new idx;
	new ip[16];
	GetPlayerIp(playerid,ip,sizeof(ip));
	while (idx < sizeof(BanInfo))
	{
	if(BanInfo[idx][BanInfoTS]!=0) {
	if(BanInfo[idx][BanInfoTS]>GetUnixTime()) {
	if(strlen(BanInfo[idx][BanInfoIP])<1) {
	    idx++;
		continue;
	}
	if(!strcmp(BanInfo[idx][BanInfoIP],ip,true)) {
		PlayerInfo[playerid][pRank]=idx;
	    return 1;
	}
	} else {
		BanInfo[idx][BanInfoTS]=0;//expired ban
	}
	}
	idx++;
	}
	return 0;
}
