public CellPhoneTimer()
{
	//if (gdebug >= 3){//printf("DEBUG CellPhoneTimer()");}
	new string[64];
	foreach (Player, i)
	{
		if(gPlayerLogged[i] == 1)
		{
			if(CellTime[i] > 0)
			{
				if (CellTime[i] == cchargetime)
				{
					CellTime[i] = 1;
					if(Mobile[Mobile[i]] == i)
					{
						CallCost[i] = callcost;
					}
				}
				////printf("CellTime %d %d",i,CellTime[i]);
				CellTime[i] = CellTime[i] +1;
				////printf("callers line %d called %d caller %d",Mobile[Mobile[i]],Mobile[i],i);
				if (Mobile[Mobile[i]] == SCRIPT_PLAYERS && CellTime[i] == 5)
				{
					new called[MAX_PLAYER_NAME];
					GetPlayerName(Mobile[i], called, sizeof(called));
					GiveNameSpace(called);
					if(PlayerInfo[Mobile[i]][pMaskUse] == 1) { format(called, sizeof(called), "Stranger_%d", RandMask[Mobile[i]]); }
					format(string, sizeof(string), "* %s's phone rings.", called);
					RingTone[Mobile[i]] = 10;
					ProxDetector(30.0, Mobile[i], string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
				}
			}
			if(CellTime[i] == 0 && CallCost[i] > 0)
			{
			    if(PlayerInfo[i][pCellplan] == 1 && PlayerInfo[i][pCellphone] > 0)
				{
				    format(string, sizeof(string), "~w~The call cost~n~~r~$%d",CallCost[i]);
				    PlayerInfo[i][pCelltime] -= CallCost[i];
				    GameTextForPlayer(i, string, 5000, 1);
				    CallCost[i] = 0;
				}
				else
				{
				    CallCost[i] = 0;
				}
			}

		}
	}
	return 1;
}

