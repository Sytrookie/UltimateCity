forward FishingCatch(playerid);
public FishingCatch(playerid)
{
    TogglePlayerControllable(playerid,true);
    if(!IsABoat(playerid))
    {
        SendClientMessage(playerid, COLOR_LIGHTRED, " You have quit the fishing mission, go sell what you received.");
		DisablePlayerCheckpoint(playerid);
		gPlayerCheckpointStatus[playerid] = CHECKPOINT_NONE;
		return 1;
    }
    new string[128];
    new Caught;
	new rand;
	new fstring[MAX_PLAYER_NAME];

	if(FishBait[playerid][1] > 0) { Caught = random(50)-20; }
    else if(FishBait[playerid][2] > 0) { Caught = random(70)-30; }
	else { SendClientMessage(playerid,COLOR_GREY," You dont have any bait to use !"); return 1; }

	rand = random(FishNamesNumber);
	if(Caught <= 0)
	{
		SendClientMessage(playerid, COLOR_GREY, "   Line snapped !");
		return 1;
	}
	else if(rand == 0)
	{
		SendClientMessage(playerid, COLOR_GREY, "   You caught a Jacket and threw it away !");
		return 1;
	}
	else if(rand == 4)
	{
		SendClientMessage(playerid, COLOR_GREY, "   You caught a Pants and threw it away !");
		return 1;
	}
	else if(rand == 7)
	{
