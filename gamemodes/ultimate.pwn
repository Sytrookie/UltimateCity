	else if(month == 10) { mtext = "October"; }
	else if(month == 11) { mtext = "November"; }
	else if(month == 12) { mtext = "December"; }
	new hour,minuite,second;
	gettime(hour,minuite,second);
	FixHour(hour);
	hour = shifthour;
	new time[128];
	format(time, sizeof(time), "%d %s %d - %d:%d",day, mtext, year, hour, minuite);
	new entry[768];
	SaveToMysql(string);
	SaveToMysql(time);
	format(entry, sizeof(entry), "INSERT INTO cmdlog (`Time`, `Name`, `ID`, `Command`) VALUES ('%s','%s',%d,'%s')",time,Name,ID,string);
