public CanPlayerUseSkin(skinid,playerid) {
	new i;
	new goodskin=1;
	new badSkins[35]= {92,52,99,3,4,5,6,8,42,65,74,86,119,149,208,268,273,289,0,102,103,104,105,106,107,108,109,110,114,115,116,173,174,175};
	for (i = 0; i < sizeof(badSkins); i++)
    {
	   	if(skinid == badSkins[i])
       	{
			return 0;
		}
	}
	if(skinid<0||skinid>299)return 0;
    if(PlayerInfo[playerid][pAdmin]>0) return 1;//admin can use anything
	for(i=0;i<sizeof(RSkinInfo);i++) {
		if(RSkinInfo[i][rskinid]==skinid) {
		if(RSkinInfo[i][rsid]==0) return 1;//not a faction skin
		if(PlayerInfo[playerid][pMember]!=RSkinInfo[i][rsid])
		{
			goodskin=0;
		} else return 1;
	}
	}
	return goodskin;
}
