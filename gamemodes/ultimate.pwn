public SyncTime()
{
	new tmphour;
	new tmpminute;
	new tmpsecond;
	gettime(tmphour, tmpminute, tmpsecond);
	FixHour(tmphour);
	tmphour = shifthour;
	if ((tmphour > ghour) || (tmphour == 0 && ghour == 23))
	{
		ghour = tmphour;
		foreach (Player, i)
		{
		    if(gPlayerLogged[i] == 1)
		    {
		        PayDayAuth[i] = 1;
		    }
		}
		if (realtime)
		{
			SetWorldTime(tmphour);
		}
		new RandomWeth;
		RandomWeth = random(3);
    	switch(RandomWeth)
    	{
        	case 0:
        	{
				DefaultWeather = 0;
				SetWeather(DefaultWeather);
        	}
        	case 1:
        	{
				DefaultWeather = 11;
				SetWeather(DefaultWeather);
        	}
        	case 2:
        	{
		    	DefaultWeather = 7;
				SetWeather(DefaultWeather);
        	}
		}
	}
}

stock ReferralSystem(playerid)
{
	new string[512];
	new result[50];
	new hours;
	new paid;
	new money;
	new ID;
    format(string, sizeof(string), "SELECT `ID`,`ReferralPaid`,`ConnectTime` FROM `players` WHERE `ReferralPaid` != `ConnectTime` AND `Referral`=%d",PlayerInfo[playerid][pID]);
	mysql_query_ex(string);
	mysql_store_result();
	if(mysql_num_rows() != 0)
	{
		mysql_retrieve_row();
	    mysql_get_field("ID", result);
		ID = strval(result);
	    mysql_get_field("ReferralPaid", result);
		paid = strval(result);
	    mysql_get_field("ConnectTime", result);
		hours = strval(result);
		if(paid < hours)
		{
		    money = (hours-paid)*500;
		}
		PlayerInfo[playerid][pActions] += money;
		money = 0;
		format(string, sizeof(string), "UPDATE `players` SET `ReferralPaid` = `ConnectTime` WHERE `ID`=%d",ID);
		mysql_query_ex(string);
		ReferralSystem(playerid);
	}
	return 1;
}

