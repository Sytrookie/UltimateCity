public CheckpointReset()
{
	//if (gdebug >= 1){//printf("DEBUG CheckpointReset()");}
	foreach (Player, i)
	{
		if(gPlayerLogged[i] == 1)
		{
			DisablePlayerCheckpoint(i);
			gPlayerCheckpointStatus[i] = CHECKPOINT_NONE;
		}
	}
	return 1;
}

public SetAllPlayerCheckpoint(Float:allx, Float:ally, Float:allz, Float:radi, num)
{
	//if (gdebug >= 1){//printf("DEBUG SetAllPlayerCheckpoint()");}
	foreach (Player, i)
	{
		if(gPlayerLogged[i] == 1)
		{
			SetPlayerCheckpoint(i,allx,ally,allz, radi);
			if (num != 255)
			{
				gPlayerCheckpointStatus[i] = num;
			}
		}
	}

}

public SetAllCopCheckpoint(Float:allx, Float:ally, Float:allz, Float:radi)
{
	//if (gdebug >= 1){//printf("DEBUG SetAllCopCheckpoint()");}
	foreach (Player, i)
	{
		if(gPlayerLogged[i] == 1)
		{
			if(gTeam[i] == 2)
			{
				SetPlayerCheckpoint(i,allx,ally,allz, radi);
			}
		}
	}
	return 1;
}

public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
    if(!IsPlayerNPC(playerid) && PRESSED(KEY_FIRE) && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerWeapon(playerid) == 34)
		ShootingWithSniper[playerid] = true;
	else if(RELEASED(KEY_FIRE))
		ShootingWithSniper[playerid] = false;
    new string[128];
	new sendername[MAX_PLAYER_NAME];
	new idcar = GetPlayerVehicleID(playerid);
	if(newkeys & KEY_FIRE && oldkeys & KEY_CROUCH && IsCbugWeapon(playerid))
	{
		if(PlayerInfo[playerid][pAdmin] <= 9)
		{
	        ApplyAnimation(playerid, "SILENCED", "Silence_reload", 3.0, 0, 0, 0, 0, 0);
	    }
	}
	if(oldkeys & KEY_FIRE && newkeys & KEY_CROUCH && IsCbugWeapon(playerid))
	{
	    if(PlayerInfo[playerid][pAdmin] <= 9)
		{
	        ApplyAnimation(playerid, "SILENCED", "Silence_reload", 3.0, 0, 0, 0, 0, 0);
	    }
	}
	else if(newkeys & KEY_FIRE)
	{
		new wep = GetPlayerWeapon(playerid);
		if(wep == 43 || wep == 44 || wep == 45)
		{
		    if(PlayerInfo[playerid][pAdmin] <= 9)
		    {
		        RemovePlayerWeaponEx(playerid, wep);
	            ApplyAnimation(playerid, "SILENCED", "Silence_reload", 3.0, 0, 0, 0, 0, 0);
	        }
	    }
	    else if(BenchPresses[playerid] > 0)
        {
		    ApplyAnimation(playerid,"benchpress","gym_bp_getoff", 4.0, 0, 0, 0, 1, 0);
		    format(string, sizeof(string), "Bench Presses: ~g~%d", BenchPresses[playerid]);
		    TextDrawSetString(Text:NewbieDraw1[playerid], string);
		    SetTimerEx("BenchPressFinish", 7000, false, "i", playerid);
        }
        else if(UsingSpawnBar[playerid] > 0 && FixSpawn[playerid] == 0)
        {
            FixSpawn[playerid] = 1;
            new house = PlayerInfo[playerid][pHousekey];
			TogglePlayerSpectating(playerid,0);
			TextDrawShowForPlayer(playerid, E1);
	        TextDrawHideForPlayer(playerid, NewbieDraw3);
	        TextDrawHideForPlayer(playerid, NewbieDraw1[playerid]);
	        TextDrawHideForPlayer(playerid, NewbieDraw2[playerid]);
			SetPlayerToTeamColor(playerid);
			SetTimerEx("SpawnBarDone", 1000, false, "i", playerid);
			SetPlayerVirtualWorld(playerid,0);
			if(UsingSpawnBar[playerid] == 1)
			{//old newbie spawn 1642.2334,-2240.4983,13.4952
			    SetPlayerPos(playerid,1662.6974,-2330.9395,6.5056); // Newbie Spawn
			}
			else if(UsingSpawnBar[playerid] == 2)
			{
			    SetPlayerPos(playerid,1480.9838,-1763.7316,18.7958); // City Hall
			}
			else if(UsingSpawnBar[playerid] == 3)
			{
			    SetPlayerPos(playerid,1470.0234,-1011.9047,26.8438); // Bank
			}
			else if(UsingSpawnBar[playerid] == 4)
			{
	            SetPlayerInterior(playerid,HouseInfo[house][hInt]);
	            SetPlayerVirtualWorld(playerid,house+100);
	            SetPlayerPos(playerid, HouseInfo[house][hExitx], HouseInfo[house][hExity],HouseInfo[house][hExitz]); // Warp the player
	            PlayerInfo[playerid][pLocal] = house;
	            PlayerInfo[playerid][pInt] = HouseInfo[house][hInt];
	            HouseEntered[playerid] = house;
		   	}
		   	else if(UsingSpawnBar[playerid] == 5)
			{
			   	if (PlayerInfo[playerid][pMember] == 1)//Police Force spawn
			   	{
					new rand;
		           	rand = random(sizeof(gCopPlayerSpawns));
