	if(!IsPlayerConnected(playerid)) return 0;

	if(gPlayerLogged[playerid] == 0) return 0;

    new t = GetTickCount();
	if(ShootingWithSniper[playerid] && (t - LastSniperShot[playerid]) > 800)
	{
	    new
			worldid = GetPlayerVirtualWorld(playerid),
			Float:x1,
