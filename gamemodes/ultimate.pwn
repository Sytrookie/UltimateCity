forward IsAtSafe(playerid);
public IsAtSafe(playerid)
{
    for(new h = 0; h < sizeof(FamilyInfo); h++)
    {
        if(PlayerToPoint(50.0, playerid, FamilyInfo[h][FamilyExit][0], FamilyInfo[h][FamilyExit][1], FamilyInfo[h][FamilyExit][2]))
        {
            if(GetPlayerVirtualWorld(playerid) == h && PlayerInfo[playerid][pMember] == h)
            {
                return 1;
            }
        }
    }
    if(PlayerInfo[playerid][pMember] == 1 && PlayerToPoint(20.0,playerid,273.5087,117.8561,1004.6172)) // Police
	{
   	    return 1;
	}
	else if(PlayerInfo[playerid][pMember] == 2 && PlayerToPoint(100.0,playerid,197.1339,170.7193,1003.0234)) // FBI
	{
   	    return 1;
	}
	else if(PlayerInfo[playerid][pMember]==4 && (PlayerToPoint(100.0,playerid,-3434.983886,478.500610,57.513174) || PlayerToPoint(100.0,playerid,1725.609985,-1645.105834,20.226491)))
	{ //EMS
	    return 1;
    }
	else if(PlayerInfo[playerid][pMember]==3 && (PlayerToPoint(100.0,playerid,356.2998,151.9914,1025.7891))) return 1; //Government
    else if(PlayerToPoint(3.0,playerid,359.6820,207.0294,1008.3828)&& PlayerInfo[playerid][pMember] == 5) return 1; //san news
    return 0;
	}

