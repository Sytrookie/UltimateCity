	else if(month == 8) { mtext = "August"; }
	else if(month == 9) { mtext = "September"; }
	else if(month == 10) { mtext = "October"; }
	else if(month == 11) { mtext = "November"; }
	else if(month == 12) { mtext = "December"; }
	new hour,minuite,second;
	gettime(hour,minuite,second);
	FixHour(hour);
	hour = shifthour;
	new entry[256];
	format(entry, sizeof(entry), "%d %s %d - %d:%d",day, mtext, year, hour, minuite);
	SaveToMysql(entry);
	new entry2[768];
	format(entry2, sizeof(entry2), "INSERT INTO paylog (`Time`, `Player`, `To`, `Amount`, `String`) VALUES ('%s', '%s', '%s', %d, '%s')",entry, Player, To, Amount, string);
	mysql_query_ex(entry2);
}

