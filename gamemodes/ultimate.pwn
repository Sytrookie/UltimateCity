public GameModeInitExitFunc()
{
	//if (gdebug >= 1){//printf("DEBUG GameModeInitExitFunc()");}
	new string[128];
	format(string, sizeof(string), "Traveling...");
	foreach (Player, i)
	{
		if(gPlayerLogged[i] == 1)
		{
			DisablePlayerCheckpoint(i);
			gPlayerCheckpointStatus[i] = CHECKPOINT_NONE;
			//if (gdebug){print("DEBUG CHECKPOINT_NONE3");}
			GameTextForPlayer(i, string, 4000, 5);
			//if (gdebug){//printf("DEBUG %s", string);}
			SetPlayerCameraPos(i,1460.0, -1324.0, 287.2);
			SetPlayerCameraLookAt(i,1374.5, -1291.1, 239.0);
		}
	}
	SetTimer("GameModeExitFunc", 4000, 0);
	return 1;
}

public OnGameModeExit()
{
	foreach (Player,i)
	{
		OnPlayerDataSave(i);
	}
	KillTimer(synctimer);
	KillTimer(newmistimer);
	KillTimer(unjailtimer);
	KillTimer(playertimer);
	KillTimer(cartimer);
	KillTimer(celltimer);
	SaveIRC();
	SaveCars();
	SaveTrunk();
	SaveSafe();
	SaveFamilies();
	SaveBans();
	OnPropUpdate();
	SaveItem();
	printf("[DEBUG]: Gamemode Restarted.");
	return 1;
}

public GameModeExitFunc()
{
	KillTimer(synctimer);
	KillTimer(newmistimer);
	KillTimer(unjailtimer);
	KillTimer(playertimer);
	KillTimer(cartimer);
	KillTimer(celltimer);
	GameModeExit();
	return 1;
}

