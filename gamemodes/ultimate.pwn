			    while ((idx < length) && (cmdtext[idx] <= ' '))
			    {
				    idx++;
			    }
			    new offset = idx;
			    new result[128];
			    while ((idx < length) && ((idx - offset) < (sizeof(result) - 1)))
			    {
				    result[idx - offset] = cmdtext[idx];
				    idx++;
			    }
			    result[idx - offset] = EOS;
			    if(!strlen(result))
			    {
				    SendClientMessage(playerid, COLOR_GRAD2, "{33CCFF}USAGE:{FFFFFF} (/announce [text]");
				    return 1;
