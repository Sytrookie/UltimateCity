stock DeleteClosestSprayTag(playerid)
{
    for(new i = 0; i < sizeof(TagInfo); i++)
  	{
  	    if(IsPlayerInRangeOfPoint(playerid, 2.0,TagInfo[i][sX], TagInfo[i][sY], TagInfo[i][sZ]))
        {
  	        if(TagInfo[i][sCreated] == 1)
            {
                TagInfo[i][sCreated]=0;
                TagInfo[i][sX]=0.0;
                TagInfo[i][sY]=0.0;
                TagInfo[i][sZ]=0.0;
                Delete3DTextLabel(TagInfo[i][sLabel]);
                TotalTags--;
                return 1;
  	        }
  	    }
	}
  	return 0;
}

stock GetDotXY(Float:StartPosX, Float:StartPosY, &Float:NewX, &Float:NewY, Float:alpha, Float:dist)
{
	 NewX = StartPosX + (dist * floatsin(alpha, degrees));
	 NewY = StartPosY + (dist * floatcos(alpha, degrees));
}

stock DeleteAllSprayTag()
{
    for(new i = 0; i < sizeof(TagInfo); i++)
  	{
   		if(TagInfo[i][sCreated] == 1)
     	{
			TagInfo[i][sCreated]=0;
			TagInfo[i][sX]=0.0;
			TagInfo[i][sY]=0.0;
			TagInfo[i][sZ]=0.0;
			Delete3DTextLabel(TagInfo[i][sLabel]);
			TotalTags = 0;
  	    }
	}
  	return 0;
}
