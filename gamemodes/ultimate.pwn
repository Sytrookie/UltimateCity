public PopulateSkinArray()
{
	new idx;
	new row[10];
	new stuff[3][5];
	printf("Loading skins from mysql.");
	new string[64];
	while (idx < sizeof(RSkinInfo))
	{
		format(string, sizeof(string), "SELECT * FROM `skins` WHERE ID=%d",idx);
	    mysql_query_ex(string);
	    mysql_store_result();
		if(mysql_num_rows() > 0)
	    {
			mysql_fetch_row(row);
			split(row, stuff, '|');
