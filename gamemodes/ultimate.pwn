}
stock SaveToMysql(String[])
{
	CharReplace(String, ''', '~');
	CharReplace(String, '`', '~');
	CharReplace(String, '|', '#');
}
stock LoadFromMysql(String[])
{
	CharReplace(String, '~', ''');
	CharReplace(String, '#', '|');
}
stock CharReplace(string[], character, replace) //n3d
{
    for(new i = strlen(string); (--i) != -1; )
        if(string[i] == character) string[i] = replace;
    return false;
}
stock GivePlayerPCash(playerid, money)
{
	PCash[playerid]+=money;
    SetPlayerMoney(playerid,PCash[playerid]);
	return 1;
}

stock ResetPlayerPCash(playerid)
{
	PCash[playerid] = 0;
	ResetPlayerMoney(playerid);
	return 1;
}

stock GetPlayerPCash(playerid)
{
	return PCash[playerid];
}

stock SetPlayerMoney(playerid,howmuch)
{
    GivePlayerMoney(playerid,howmuch-GetPlayerMoney(playerid));
    return 1;
}

